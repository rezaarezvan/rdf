\usetikzlibrary{positioning}

\begin{tikzpicture}[>=stealth, node distance=20mm, thick]

% Styles
    \tikzstyle{v}=[circle, draw=black, minimum size=20mm, font=\LARGE\bfseries, line width=0.8mm]
    \tikzstyle{arrow}=[->, black, line width=0.8mm]
    \tikzstyle{b}=[circle, draw=black, fill=blue!20, minimum size=20mm, font=\LARGE\bfseries, line width=0.8mm]

% Nodes
\node[v] (x1) {$x_1$};
