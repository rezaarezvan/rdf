\node[b, right=20mm of x5] (x6) {$x_6$};

% Arrows
\draw[arrow] (x1) -- (x2);
\draw[arrow] (x2) -- (x3);
\draw[arrow] (x3) -- (x4);
\draw[arrow] (x4) -- (x5);
